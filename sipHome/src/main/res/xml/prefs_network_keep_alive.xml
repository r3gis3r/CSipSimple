<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android" >

    <!-- About keep alives -->
    <!-- UDP -->
    <EditTextPreference
        android:defaultValue="100"
        android:key="keep_alive_interval_wifi"
        android:numeric="signed"
        android:summary="@string/keep_alive_interval_desc"
        android:title="@string/udp_keep_alive_interval_wifi" />
    <EditTextPreference
        android:defaultValue="40"
        android:key="keep_alive_interval_mobile"
        android:numeric="signed"
        android:summary="@string/keep_alive_interval_desc"
        android:title="@string/udp_keep_alive_interval_mobile" />
    <!-- TCP -->
    <EditTextPreference
        android:defaultValue="180"
        android:key="tcp_keep_alive_interval_wifi"
        android:numeric="signed"
        android:summary="@string/keep_alive_interval_desc"
        android:title="@string/tcp_keep_alive_interval_wifi" />
    <EditTextPreference
        android:defaultValue="120"
        android:key="tcp_keep_alive_interval_mobile"
        android:numeric="signed"
        android:summary="@string/keep_alive_interval_desc"
        android:title="@string/tcp_keep_alive_interval_mobile" />
    <!-- TLS -->
    <EditTextPreference
        android:defaultValue="180"
        android:key="tls_keep_alive_interval_wifi"
        android:numeric="signed"
        android:summary="@string/keep_alive_interval_desc"
        android:title="@string/tls_keep_alive_interval_wifi" />
    <EditTextPreference
        android:defaultValue="120"
        android:key="tls_keep_alive_interval_mobile"
        android:numeric="signed"
        android:summary="@string/keep_alive_interval_desc"
        android:title="@string/tls_keep_alive_interval_mobile" />

</PreferenceScreen>