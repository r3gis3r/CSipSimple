<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android" >

    <CheckBoxPreference
        android:defaultValue="true"
        android:key="force_no_update"
        android:summary="@string/force_no_update_desc"
        android:title="@string/force_no_update" />
    <CheckBoxPreference
        android:defaultValue="false"
        android:key="add_bandwidth_tias_in_sdp"
        android:summary="@string/add_bandwidth_tias_in_sdp_desc"
        android:title="@string/add_bandwidth_tias_in_sdp" />
    <CheckBoxPreference
        android:defaultValue="false"
        android:key="disable_rport"
        android:summary="@string/disable_rport_desc"
        android:title="@string/disable_rport" />

    <!-- Timers -->
    <EditTextPreference
        android:defaultValue="90"
        android:key="timer_min_se"
        android:numeric="signed"
        android:summary="@string/timer_min_se_desc"
        android:title="@string/timer_min_se" />
    <EditTextPreference
        android:defaultValue="1800"
        android:key="timer_sess_expires"
        android:numeric="signed"
        android:summary="@string/timer_sess_expires_desc"
        android:title="@string/timer_sess_expires" />
    <EditTextPreference
        android:defaultValue="-1"
        android:key="tsx_t1_timeout"
        android:numeric="signed"
        android:summary="@string/tsx_timeout_desc"
        android:title="@string/tsx_t1_timeout" />
    <EditTextPreference
        android:defaultValue="-1"
        android:key="tsx_t2_timeout"
        android:numeric="signed"
        android:summary="@string/tsx_timeout_desc"
        android:title="@string/tsx_t2_timeout" />
    <EditTextPreference
        android:defaultValue="-1"
        android:key="tsx_t4_timeout"
        android:numeric="signed"
        android:summary="@string/tsx_timeout_desc"
        android:title="@string/tsx_t4_timeout" />
    <EditTextPreference
        android:defaultValue="-1"
        android:key="tsx_td_timeout"
        android:numeric="signed"
        android:summary="@string/tsx_timeout_desc"
        android:title="@string/tsx_td_timeout" />
    <EditTextPreference
        android:defaultValue="0"
        android:key="thread_count"
        android:numeric="signed"
        android:summary="@string/thread_count_desc"
        android:title="@string/thread_count" />

</PreferenceScreen>